# Open web browser
super + w
	$BROWSER

Menu
	$TERM_EMULATOR

# Open file manager
super + r
	$TERM_EMULATOR -e ranger

# Open gvim
super + e
	gvim

# Open calendar (calcurse)
super + c
	$TERM_EMULATOR -e calcurse

# Switch to previuos keyboard layout
super + space
	keyboard_layout --prev && sigdwmblocks 2

# Choose a keyboard layout
super + control + space
	keyboard_layout --set && sigdwmblocks 2

# System options
super + shift +q
	dmenu_system

# Pipe clipboard to a program
super + shift + backslash
	dmenu_clipboard_run

# Take a screenshot
Print
	dmenu_screenshot

# Wifi options
super + F1
	dmenu_wifi

# Edit common config files
super + F2
	dmenu_editconfig

# Passmenu
super + F3
	passmenu

# Rofi window switch
alt + Tab
    rofi -show window

# Edit scripts
super + F4
	$TERM_EMULATOR -e fzf_editscripts

# Kill a process with dmenu
super + F9
	dmenu_kill

# Start dmenu_run with history
#super + shift + p
#	dmenu_run_history

# Rofi drun
super + shift + p
    rofi -show drun

# Mount a drive
super + F12
	dmenu_mount

XF86Calculator
	st -c floating_st -e python3 -q

XF86Mail
	sxiv -g 1100x1100 -sf -b ~/Downloads/conversions.png

super + i
	zathura ~/Downloads/kuiper_interfaces_evaluation.pdf

# Unmount a drive
super + F11
	dmenu_unmount

# Open bluetoothctl terminal
super + alt + b
	$TERM_EMULATOR -e bluetoothctl

# Audio control
XF86AudioMute
    amixer -D pulse set Master 1+ toggle

super + alt + m
    amixer -D pulse set Master 1+ toggle

XF86Audio{Raise,Lower}Volume
	amixer -D pulse sset Master 5%{+,-}

XF86MonBrightness{Up,Down}
	xbacklight {+,-} 10

super + alt + {k,j}
	amixer -D pulse sset Master 5%{+,-}

super + alt + v
    pavucontrol

alt + o; g
	chromium https://gitlab.com/irena1/kuiper_gw_modem/-/tree/pynq_verification

alt + o; 7; {2,3}
	chromium 192.168.0.7{2,3}:9090/lab?
